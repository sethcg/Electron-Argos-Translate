// THIS IS THE TYPESCRIPT CONFIG FOR ./viteconfig FILES
// *FROM MY UNDERSTANDING THEY NEED TO BE BUNDLED AS MODULES*
{
  "compilerOptions": {
    "composite": true,
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowSyntheticDefaultImports": true
  },
  "include": ["viteconfig/**/*"]
}
